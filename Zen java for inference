import java.io.IOException;
import java.nio.file.Files;
import java.nio.file.Paths;

public class StressDetection {
    public static void main(String[] args) {
        // Load the model
        try (Graph graph = new Graph()) {
            byte[] graphDef = Files.readAllBytes(Paths.get("stress_detection_model.h5"));
            graph.importGraphDef(graphDef);

            try (Session session = new Session(graph)) {
                // Prepare input tensor (example: replace with actual image data)
                float[][][][] inputData = new float[1][48][48][1]; // Adjust based on your input shape
                // Fill inputData with actual image data here

